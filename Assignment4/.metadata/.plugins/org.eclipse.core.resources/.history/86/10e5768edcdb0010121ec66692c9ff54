#include "gpio.h"





#if !defined(__SOFT_FP__) && defined(__ARM_FP)
  #warning "FPU is not initialized, but the project is compiling for an FPU. Please initialize the FPU before use."
#endif

int main(void)
{
	int count=0;
	led_init();
	switch_init();
	while(1)
	{
		if (switch_status() == 1 )
		{
			count++;
			if(count==1)
			{
				GPIOD->ODR|=(1<<12);
				GPIOD->ODR &= ~(1<<15);
			}
			else if(count==2)
			{
				GPIOD->ODR &= ~(1<<12);
				GPIOD->ODR |=(1<<15);
			}
			else if(count==3)
					{
						GPIOD->ODR &= ~((1<<12)|(1<<15));
						count==0;

					}
			DelayMs(300);
			while(switch_status()==1);




		}
	}
}







